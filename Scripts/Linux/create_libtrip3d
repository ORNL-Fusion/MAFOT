names=( circleb ellipints biotloop polygonb d3pferr d3ccoils d3icoils itericoilsgeom nstxecgeom )

for ((i=0; i<${#names[*]}; i++))
do
echo -n "$((i+1)): ${names[$i]}  "
gfortran -c ${names[$i]}.f -O3 -fPIC
if [ $? = 0 ]
  then echo Success
fi
ar ruvs libtrip3d.a ${names[$i]}.o
mv ${names[$i]}.o ./obj/
done

